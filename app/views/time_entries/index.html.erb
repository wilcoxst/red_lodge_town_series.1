<h1>Listing time_entries</h1>

<table>
  <thead>
    <tr>
      <th>Timing</th>
      <th>Run</th>
      <th>Week</th>
      <th>Racer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @time_entries.each do |time_entry| %>
      <tr>
        <td><%= time_entry.timing %></td>
        <td><%= time_entry.run %></td>
        <td><%= time_entry.week %></td>
        <td><%= time_entry.racer %></td>
        <td><%= link_to 'Show', time_entry %></td>
        <td><%= link_to 'Edit', edit_time_entry_path(time_entry) %></td>
        <td><%= link_to 'Destroy', time_entry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Time entry', new_time_entry_path %>
