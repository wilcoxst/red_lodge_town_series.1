<%- model_class = TimeEntry -%>
<div class="page-header">
  <h1><%=t '.title', :default => TimeEntry %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:timing) %></dt>
      <dd><pre class="prettyprint"><%= @time_entry.timing %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:run) %></dt>
      <dd><pre class="prettyprint"><%= @time_entry.run %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              time_entries_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_time_entry_path(@time_entry), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              time_entry_path(@time_entry),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

