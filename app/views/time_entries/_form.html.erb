<%= simple_form_for(@time_entry) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :timing %>
    <%= f.input :run %>
    <%= f.association :week %>
    <%= f.association :racer %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
